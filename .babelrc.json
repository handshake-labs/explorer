{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime"],
    [
      "react-css-modules",
      {
        "handleMissingStyleName": "ignore",
        "generateScopedName": "[path]__[name]__[local]",
        "autoResolveMultipleImports": true
      }
    ]
  ],
  "sourceMaps": true
}
